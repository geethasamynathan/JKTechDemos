@model  IEnumerable<DemoMVCApp1.Models.Employee>
 

@{
    ViewBag.Title = "Index";
}

<h2> Employee List</h2>
<table border="1">
    <tr>
        <th> Id</th>
        <th> Name</th>
        <th> Designation</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td> @item.Name</td>
            <td>@item.Designation</td>
        </tr>
    }
</table>